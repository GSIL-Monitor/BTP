    <script type="text/javascript">

        var bakUrl = window.location.href;
        function getBtpDomain() {
            return "http://" + window.location.host + "/";
        }
        function search_click() {
            sessionStorage.ComTypeSearch = 0;
            sessionStorage.page = 1;
            getWantCommodityData($('#nav_search_txt').val());
        }
        var wxOpenId = getQueryString('wxOpenId');
        if (JsVilaDataNull(wxOpenId)) {
            sessionStorage.wxOpenId = wxOpenId;
        }

        if (getQueryString('setCategoryId') != "" && getQueryString('setCategoryId') != null) {
            sessionStorage.categoryId = getQueryString('setCategoryId');
        } else {
            sessionStorage.categoryId = 'C9F33B9D-D90A-4634-B882-42CB8BC8B131';
        }
        
        if (window.location.href.toLowerCase().indexOf("setmobile") < 0 || window.location.href.indexOf("?") < 0) {
            sessionStorage.source = "share";
        }
        if (JsVilaDataNull(getQueryString('source'))) {
            sessionStorage.source = getQueryString('source');
        }
        var srcAppId = getQueryString("srcAppId");
        if (JsVilaDataNull(getQueryString("srcAppId"))) {
            sessionStorage.srcAppId = getQueryString("srcAppId");
        }
        if (sessionStorage.source != "share") {
            //厂家直营才会有此参数 根据此参数 判断是否是 厂家直营
            sessionStorage.ProductType = "appcjzy";
            sessionStorage.AppType = "tuwen";
            if (sessionStorage.SrcType != 39 && sessionStorage.SrcType != 40) {
                sessionStorage.SrcType = 36;
            }
        } else {
            sessionStorage.ProductType = "webcjzy";
            sessionStorage.AppType = "tuwen";
            sessionStorage.SrcType = 34;
        }


        var cjzy = getQueryString('cjzy');
        if (cjzy == "1") {
            sessionStorage.ProductType = "webcjzy";
            sessionStorage.AppType = "tuwen";
        } else if (cjzy == "0") {
            sessionStorage.ProductType = "appcjzy";
            sessionStorage.AppType = "tuwen";
        }

        if (JsVilaDataNull(getQueryString("SrcType"))) {
            sessionStorage.SrcType = getQueryString('SrcType');
        }

        var srcTagId = getQueryString('SrcTagId');

        if (srcTagId != undefined && srcTagId != "" && srcTagId != "null" && srcTagId != null && srcTagId != "undefined") {

            sessionStorage.SrcTagId = getQueryString('SrcTagId');
        }
        var cpsId = getQueryString('CPSId');

        if (cpsId != undefined && cpsId != "" && cpsId != "null" && cpsId != null && cpsId != "undefined") {
            sessionStorage.CPSId = getQueryString('CPSId');
        }

      
        if (JsVilaDataNull(getQueryString("productType"))) {
            sessionStorage.ProductType = getQueryString("productType");
        }
        if (JsVilaDataNull(getQueryString("speader"))) {
            sessionStorage.speader = getQueryString("speader");
        }


        $(function () {
            saveContextDTOByUrl();
            var psource = sessionStorage.source == "share" ? "&source=share" : "";
            if (sessionStorage.appId && sessionStorage.appId != "undefined" && sessionStorage.appId != "null") {
            }
            else {
                sessionStorage.appId = getQueryString('appId');
            }
            if (!sessionStorage.appId || sessionStorage.appId == "null" || sessionStorage.appId == "undefined") {
                sessionStorage.appId = sessionStorage.appIdCong;
            }
            
            if (getQueryString('type') == "tuwen") {
                sessionStorage.AppType = "tuwen";
                $(".footdi").show();
                if (sessionStorage.appId.toUpperCase() == "bb545ae5-d3ac-47ca-96a5-a8bf002bfd08".toUpperCase()) {
                    $("#promotion").hide();
                    $("#guang span").text("报刊订阅");
                    $("#footul li").css("width", "27%");
                    $("#search").parent().css("width", "11%");
                }
                else {

                }
                if (sessionStorage.appName != undefined && sessionStorage.appName != "") {
                    document.title = sessionStorage.appName;
                }
            }
            //
            $("#dwangpwd").bind("click", function () {
                window.location.href = "@ViewBag.PortalUrl" + "/Phone/WebRetrievePwd.html?url=" + bakUrl;
            });

            var sharepar = "&SrcType=" + sessionStorage.SrcType + "&share=" + getShareId() + "&source=" + sessionStorage.source;


            var pwxOpenId = JsVilaDataNull(sessionStorage.wxOpenId) ? "&wxOpenId=" + sessionStorage.wxOpenId : "";
            $("#guang").attr("href", "/SetMobile/Index?setCategoryId=C9F33B9D-D90A-4634-B882-42CB8BC8B131&pageIndex=1&pageSize=10&fieldSort=0&order=1" + psource + pwxOpenId);
            $("#myhome").attr("href", "/Mobile/MyHome?vc=vc&type=tuwen" + sharepar);
            $("#shopcart").attr("href", "/Mobile/ShoppongCartList?appId=" + sessionStorage.appId + "&type=tuwen" + sharepar);
            $("#searchZPH").attr("href", "/SetMobile/ZPHCategory?appId=" + sessionStorage.appId + "&type=tuwen" + sharepar);

            $("#_search").bind("click", function () {
                var aa = $("#Commodity_nav").css("left");
                if (aa.indexOf("-") >= 0) {
                    $("#Commodity_nav").css({ position: "fixed", display: "block", left: "0px", top: "50px", bottom: "0" });
                    navBackground();
                } else {
                    var navWidth = $("#Commodity_nav").css({ position: "absolute", visibility: "hidden", display: "block" }).width();
                    $("#Commodity_nav").css({ position: "fixed", visibility: "inherit", display: "none", left: -navWidth });
                    $("#navBackground").remove();
                }
            });
        });




        function footmenu(obj) {
            var type = obj.attr("id");
            switch (type) {
                case "guang":
                    obj.find("i").removeClass("footer-itemIco-guang");
                    obj.find("i").addClass("footer-itemIco-guang-on");
                    break;
                case "shopcart":
                    obj.find("i").removeClass("footer-itemIco-shop");
                    obj.find("i").addClass("footer-itemIco-shop-on");
                    break;
                case "myhome":
                    obj.find("i").removeClass("footer-itemIco-order");
                    obj.find("i").addClass("footer-itemIco-order-on");
                    break;
                case "searchZPH":
                    obj.find("i").removeClass("footer-itemIco-searchZPH");
                    obj.find("i").addClass("footer-itemIco-searchZPH-on");
                    break;
                default:
                    break;
            }
        }
        $(function () {
            $("#nav_search_txt")[0].onkeydown = function (event) {
                var e = event || window.event || arguments.callee.caller.arguments[0];
                if (e && e.keyCode == 13) {
                    search_click();
                    this.blur();
                }
            };
            $('#hTitle').show();
            $('#nav_search_txt').hide();
            $('#divSearch').hide();
        });

    </script>
