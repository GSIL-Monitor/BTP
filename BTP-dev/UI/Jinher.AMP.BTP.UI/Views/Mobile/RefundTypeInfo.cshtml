@section ClientScript
{
    <script src="/Scripts/md5.js" type="text/javascript"></script>
    <script type="text/javascript">
        $(function () {
            saveContextDTOByUrl();
            if (!isWeiXin() && sessionStorage.source == "share") {
                $(".mobile-header").show();
            } else {
                $(".mobile-header").hide();
            }
            $('#RefundExpCo').text(unescape(getQueryString('RefundExpCo')));
            $('#RefundExpNo').text(unescape(getQueryString('RefundExpNo')));
        });

        function CancelSubmit() {
            window.location.href = urlAppendCommonParams("/Mobile/MyOrderDetail?orderId=" + getQueryString('orderId') + "&shopId=" + sessionStorage.appId);
        }        
      
    </script>
}
<header class="mobile-header" style="display: none">
	<div class="icon-no" style="left:auto;right:13px;" onclick="CancelSubmit()"></div>
	<h1 class="page-name">退货方式</h1>
	
</header>
<div class="content">
    <ul class="list">
        <li>
            <div class="item">
                <span>物流公司</span><span id="RefundExpCo"></span></div>
            <div class="item">
                <span>物流单号</span><span id="RefundExpNo"></span></div>
        </li>
    </ul>
</div>
