        function toNoInSale(commodityIds) {
            $.ajax({
                url: '/Commodity/Shelves',
                type: 'post',
                data: { CommodityIds: commodityIds },
                error: function () {
                    alert("请求错误，请稍后刷新进行操作.");
                },
                success: function (msg) {
                    alert("上架成功!");
                    window.location.href = "/Commodity/StoreIndex?appId=" + getQueryString('appId');
                }
