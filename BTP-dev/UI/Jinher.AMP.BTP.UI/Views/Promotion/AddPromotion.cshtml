            $("#btnSubmit").bind("click", function () {
                var postData = {};
                postData.PromotionName = $("#PromotionName").val();
                postData.StartTime = $("#StartTime").val();
                postData.EndTime = $("#EndTime").val();

                switch ($("input[name=disc]:checked").attr("id")) {
                    case "rdIntensity":
