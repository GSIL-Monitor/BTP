                }
                if ($("#Intensity").val() <= 0) {
                    alert("请填写正确的折扣");
                }
                if (!/^\d+(?=\.{0,1}\d$|$)/.test($("#Intensity").val())) {
                    alert("请填写正确的折扣");
                }
                if ($("#Intensity").val() >= 10) {
                    alert("请填写正确的折扣");
                }

