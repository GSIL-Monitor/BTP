                $("#chkPartialFree").removeAttr("checked");
                $("#partialFreeTable").hide();
            }

            if(this._pfListJson != null && this._pfListJson.length > 0)
            {
                var pfLen = this._pfListJson.length;
                for(var i= 0; i < pfLen;i++)
                {
