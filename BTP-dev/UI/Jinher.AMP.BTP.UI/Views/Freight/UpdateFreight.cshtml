            $(e).parent("div").find('div[af-tag*="freightFree"]').hide();
            $(e).parent("div").find('div[af-tag="freightFree' + ffType + '"]').show();
        }
        
        //计算区间运费
        function computeFreights(freightElements, provinceCodes, specific) {

            if(!specific){
                specific = false;
            }

            if(!provinceCodes){
                provinceCodes = "";
            }

            var freights = [];
            if (freightElements) {

                $.each(freightElements, function(index, row){
                    var $this = $(this)
                        , schema = { 
                            provinceCodes: provinceCodes
                            , isSpecific: specific
                            , min: 0
                            , max: 0
                            , cost: 0 
                         };
                    schema.min = parseInt($this.children().eq(1).children().first().val());
                    schema.max = parseInt($this.children().eq(3).children().first().val());
                    schema.cost = parseInt($this.children().eq(4).children().first().val());

                    freights.push(schema);
                });
            }
            return freights;
        }

        //添加一组默认运费的区间费用
        function addDefaultRangeCost(sender) {

            var $sender = $(sender)
                , $table = $sender.closest("table")
                , $container = $table.find("tbody")
                , $template = $table.find("tfoot").children().eq(1)
                , nowCount = $container.children().length;

            if (nowCount >= 10) {
                alert("最多可设置10个层级");
                return;
            }

            $template.clone().appendTo($container).removeAttr("class style").show().children().eq(0).text(++nowCount);
        }

        //移除一组默认运费的区间费用
        function removeDefaultRangeCost(sender) {

            if (!confirm("确定要删除吗？")) {
                return;
            }

            var $sender = $(sender)
                , $targetTr = $sender.parent().parent()
                , $siblings = $targetTr.siblings();

            $targetTr.remove();

            $siblings.each(function (index) {
                $(this).children().eq(0).text(index + 1);
            });
        }

        function removeSpecificGroup(sender){
            var groupCls = ".specific-freight-group"
                , $sender = $(sender);

            if (confirm("确定要删除吗？")) {

                var $group = $sender.closest(".specific-freight-group");

                var index = $.inArray($group.attr("id"), groupIds);
                if (index > -1) {
                    groupIds.splice(index, 1);
                }
                $sender.closest(groupCls).remove();
            }
        }

        function createId(prefix) {
            return prefix + Math.random().toString().substring(2);
        }

        //添加一组特定地区的运费模板
        function addSpecificAreaFreight(sender) {

            var groupId = createId("specificArea")
                , $template = $("#specificGroupTemplate")
                , $container = $("li.areaFreight").find(".container")
                , $table = null;

            var $clone = $template.clone();

            groupIds.push(groupId);

            $table = $clone.children().last();

            $table.children().first().find("a").click(function () { OpenProvinceDiv(this, groupId); });

            $container.append($clone.removeAttr("style").attr({ "id": groupId }).addClass("groupFreight").show());
        }

        function loadRangeTemplate(){
            
            console.log("fn:load-range-template");

            var $container = $(".areaFreight").find(".container");

            if($container.children().length > 0){
                $container.parent().show();
                $container.children().each(function(){
                    var groupId = createId("update-specific");
                    groupIds.push(groupId);
                    $(this).attr({ id: groupId }).show().children().last().children().first().find("a").click(function () { OpenProvinceDiv(this, groupId); });
                });
            }
        }
    </script>
