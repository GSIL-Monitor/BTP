    List<Jinher.AMP.BTP.Deploy.CustomDTO.FreightPartialFreeExtDTO> pfList = freight.PartialFreeList;

    var ffCount = freight.FirstCount;
    var fnCount = freight.NextCount;
    if (freight.PricingMethod == 0)
    {
        ffCount = decimal.Round(freight.FirstCount, 0);
        fnCount = decimal.Round(freight.NextCount, 0);
    }


    JavaScriptSerializer js = new JavaScriptSerializer();

    string pfListJsonString = js.Serialize(pfList);
    pfListJsonString = HttpUtility.UrlEncode(pfListJsonString);


}
